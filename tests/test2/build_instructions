./asembler -o ./tests/test3/handler.o ./tests/test3/handler.s
./asembler -o ./tests/test3/main.o ./tests/test3/main.s
./linker -hex -place=my_code_main@0x40000000 -place=my_code_handler@0xC0000000 -o ./tests/test3/program.hex ./tests/test3/handler.o ./tests/test3/main.o
./emulator ./tests/test3/program.hex